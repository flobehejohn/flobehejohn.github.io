    <script>
      document.addEventListener('DOMContentLoaded', () => {
        document.querySelectorAll('.carte-lecteur-video video').forEach(video => {
          // Cible : timecode à la 3e image (~0,125s pour 24fps)
          const frameTime = 0.125;
      
          // Fonction pour forcer l'affichage à la 3e frame dès le chargement
          function seekToFrame() {
            // Si la vidéo n'est pas déjà à la bonne image, on la positionne
            if (Math.abs(video.currentTime - frameTime) > 0.01) {
              video.currentTime = frameTime;
            }
          }
      
          // 1. À l'arrivée des métadonnées
          video.addEventListener('loadedmetadata', seekToFrame);
      
          // 2. Certains navigateurs ont besoin d'attendre l'événement "seeked" pour afficher la bonne image
          video.addEventListener('seeked', function onceSeeked() {
            // Ne rien faire d'autre : l'image est affichée
            // On retire ce listener après le premier déclenchement
            video.removeEventListener('seeked', onceSeeked);
          });
      
          // 3. Pour Safari/iOS ou cas particuliers, retenter après un court délai
          setTimeout(() => {
            seekToFrame();
          }, 150);
        });
      });
      </script>
